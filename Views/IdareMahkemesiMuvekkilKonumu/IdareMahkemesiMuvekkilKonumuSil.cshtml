@model IdareMahkemesiMuvekkilKonumu

@{
    ViewData["Title"] = "İdare Mahkemesi Muvekkil Konumu Düzenle";
}



@section Styles{
    <link href="~/css/muvekkil.css" rel="stylesheet" />
}

<h3 class="muvekkilSilH3">İdare Mahkemesi Müvekkil Konumunu Silmek İstediğinize Emin Misiniz?</h3>
<div class="muvekkilWrapper">
    <div class="muvekkilEkleHeader">
        <label class="muvekkilEkleHeaderTitle">İdare Mahkemesi Müvekkil Konumu Sil</label>
    </div>
    <div class="muvekkilEkleContent">
        <form asp-controller="IdareMahkemesiMuvekkilKonumu" asp-action="IdareMahkemesiMuvekkilKonumuSil" method="post">
            <div class="muvekkilEkleContentRow1">
                <div class="form-group">
                    <label asp-for="idareMahkemesiMuvekkilKonumuTuru" class="control-label" readonly></label>
                    <input asp-for="idareMahkemesiMuvekkilKonumuTuru" class="form-control" readonly/>
                    <span asp-validation-for="idareMahkemesiMuvekkilKonumuTuru" class="text-danger" readonly></span>
                </div>
                <div class="muvekkilEkleContentRow5">
                    <button class="btn btn-primary ">Sil</button>
                    <button asp-controller="Ayarlar" asp-action="YoneticiAyarlariListele" class="btn btn-danger">İptal</button>
                </div>
            </div>
        </form>
    </div>
</div>