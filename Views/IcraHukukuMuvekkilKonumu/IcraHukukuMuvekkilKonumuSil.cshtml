@model IcraHukukuMuvekkilKonumu

@{
    ViewData["Title"] = "Icra Hukuku Muvekkil Konumu Düzenle";
}



@section Styles{
    <link href="~/css/muvekkil.css" rel="stylesheet" />
}

<h3 class="muvekkilSilH3">İcra Hukuku Müvekkil Konumunu Silmek İstediğinize Emin Misiniz?</h3>
<div class="muvekkilWrapper">
    <div class="muvekkilEkleHeader">
        <label class="muvekkilEkleHeaderTitle">İcra Hukuku Müvekkil Konumu Sil</label>
    </div>
    <div class="muvekkilEkleContent">
        <form asp-controller="IcraHukukuMuvekkilKonumu" asp-action="IcraHukukuMuvekkilKonumuSil" method="post">
            <div class="muvekkilEkleContentRow1">
                <div class="form-group">
                    <label asp-for="IcraHukukMuvekkilKonumu" class="control-label" readonly></label>
                    <input asp-for="IcraHukukMuvekkilKonumu" class="form-control" readonly/>
                    <span asp-validation-for="IcraHukukMuvekkilKonumu" class="text-danger" readonly></span>
                </div>
                <div class="muvekkilEkleContentRow5">
                    <button class="btn btn-primary ">Sil</button>
                    <button asp-controller="Ayarlar" asp-action="YoneticiAyarlariListele" class="btn btn-danger">İptal</button>
                </div>
            </div>
        </form>
    </div>
</div>