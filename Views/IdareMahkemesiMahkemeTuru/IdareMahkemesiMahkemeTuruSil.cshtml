@model IdareMahkemesiMahkemeTuru

@{
    ViewData["Title"] = "Idare Mahkemesi Mahkeme Turu Ekle";
}


@section Styles{
    <link href="~/css/muvekkil.css" rel="stylesheet" />
}

<h3 class="muvekkilSilH3">Idare Mahkemesi Mahkeme Turu Silmek İstediğinize Emin Misiniz?</h3>
<div class="muvekkilWrapper">
    <div class="muvekkilEkleHeader">
        <label class="muvekkilEkleHeaderTitle">Idare Mahkemesi Mahkeme Turu Ekle Sil</label>
    </div>
    <div class="muvekkilEkleContent">
        <form asp-controller="IdareMahkemesiMahkemeTuru" asp-action="IdareMahkemesiMahkemeTuruSil" method="post">
            <div class="muvekkilEkleContentRow1">
                <div class="form-group">
                    <label asp-for="idareMahkemesiMahkemeTuruu" class="control-label" readonly></label>
                    <input asp-for="idareMahkemesiMahkemeTuruu" class="form-control" readonly />
                    <span asp-validation-for="idareMahkemesiMahkemeTuruu" class="text-danger" readonly></span>
                </div>
                <div class="muvekkilEkleContentRow5">
                    <button class="btn btn-primary ">Sil</button>
                    <button asp-controller="Ayarlar" asp-action="YoneticiAyarlariListele" class="btn btn-danger">İptal</button>
                </div>
            </div>
        </form>
    </div>
</div>
