@model IdareMahkemesiMahkemeTuru

@{
    ViewData["Title"] = "Idare Mahkemesi Mahkeme Turu Duzenle";
}



@section Styles{
    <link href="~/css/muvekkil.css" rel="stylesheet" />
    <style>
        .headerBackButton {
            transition: 175ms all ease-in-out;
        }

        .headerBackButton:hover {
            font-size: 16px;
        }
    </style>
}

<div class="muvekkilWrapper">
    <div class="muvekkilEkleHeader">
        <a asp-controller="Ayarlar" asp-action="YoneticiAyarlariListele" style="margin-right:15px; color:#333;" class="headerBackButton"><i class="fas fa-chevron-left ki"></i></a>
        <label class="muvekkilEkleHeaderTitle">Idare Mahkemesi Mahkeme Turu Duzenle</label>
    </div>
    <div class="muvekkilEkleContent">
        <form asp-controller="IdareMahkemesiMahkemeTuru" asp-action="IdareMahkemesiMahkemeTuruDuzenle" method="post">
            <div class="muvekkilEkleContentRow1">

                <div class="form-group">
                    <label asp-for="idareMahkemesiMahkemeTuruu" class="control-label"></label>
                    <input asp-for="idareMahkemesiMahkemeTuruu" class="form-control" />
                    <span asp-validation-for="idareMahkemesiMahkemeTuruu" class="text-danger"></span>
                </div>
                <div class="muvekkilEkleContentRow5">
                    <button class="btn btn-primary ">Düzenle</button>
                    <button asp-controller="Ayarlar" asp-action="YoneticiAyarlariListele" class="btn btn-danger">İptal</button>
                </div>
                <div class="form-group">
                    <input asp-for="idareMahkemesiMahkemeTuruId" class="form-control" hidden readonly />
                </div>
            </div>
        </form>
    </div>
</div>

